<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/service-definition-2.1.xsd">
    <service verb="create" noun="Example" type="inline">
        <in-parameters>
            <auto-parameters include="all"/>
        </in-parameters>
        <out-parameters>
            <auto-parameters include="pk" required="true"/>
        </out-parameters>
        <actions>
            <entity-make-value entity-name="helloworld-component.Example" value-field="tutorial"/>
            <entity-set value-field="tutorial" include="all"/>
            <if condition="!tutorial.tutorialId">
                <entity-sequenced-id-primary value-field="tutorial"/>
            </if>
            <entity-create value-field="tutorial"/>
		<transition name="createTutorial">
    <service-call name="tutorial.TutorialServices.create#Example"/>
    <default-response url="."/>
</transition>
        </actions>
    </service>
</services>
